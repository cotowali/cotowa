fn git(command string) string
fn add(a int, b int) int {
  return a + b
}

fn div(a float, b float) float {
  return a / b
}

fn sum(vals ...int) int {
  let res = 0
  for v in vals {
    res = res + v
  }
  return res
}

assert add(1, 2) == 3
assert div(1, 2) == 0.5
assert sum(1, 2, 3) == 6

assert @expr('1', '+', '1') == '2'
