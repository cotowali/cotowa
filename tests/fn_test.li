fn test_nested_func() {
  fn f(x: string): string {
    fn f(v: int): int {
      return 1 + v
    }
    var v = f(10)
    assert(v == 11)
    return "$x $v"
  }

  var v = f('v')
  assert(v == 'v 11')
}
test_nested_func()

fn test_varargs_array_method() {
  fn varargs_join(sep: string, vals: ...string): string {
    return vals.join(sep)
  }
  assert(varargs_join(':', 'a', 'b', 'c') == 'a:b:c')
}
test_varargs_array_method()
