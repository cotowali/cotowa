fn git(command: string) string
fn add(a: int, b: int) int {
  return a + b
}

fn div(a: float, b: float) float {
  return a / b
}

fn sum(vals: ...int) int {
  var res = 0
  for v in vals {
    res = res + v
  }
  return res
}

assert add(1, 2) == 3
assert div(1, 2) == 0.5
assert sum(1, 2, 3) == 6
assert sum(1, 2, 3,) == 6

assert @expr('1', '+', '1') == '2'
assert ((1.1 |> @awk('{print $1 * 2}')) as float) == 2.2
