assert typeof([0]) == '[]int'
assert typeof({ 'k': 0 }) == 'map[string]int'
assert typeof([
  [
    {
      'k': [{ 'k': map[string][]int{} }]
    }
  ]
]) == '[][]map[string][]map[string]map[string][]int'
