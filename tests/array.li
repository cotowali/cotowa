fn index_test() {
  var arr = [10, 11, 12]
  assert arr[0] == 10
  assert arr[1] == 11
  arr[2] = 100
  assert arr[2] == 100
  assert arr[
    2
  ] == 100

  arr = [10, 11, 12]
  var i = 0
  assert arr[i] == 10
  i = 2
  assert arr[i] == 12
  arr[i] = 0
  assert arr[i] == 0
}
index_test()

fn array_of_array_test() {
  var v1 = [[1, 2], [3, 4]]
  assert v1[0][0] == 1
  assert v1[1][1] == 4

  var v2 = [[['a']]]
  assert v2[0][0][0] == 'a'
}

fn array_of_tuple_test() {
  var arr = [(0, 1), (2, 3)]
  echo('array of tuple for')
  for v in arr {
    echo("v: $v")
  }
}
array_of_tuple_test()
