fn index_test() {
  var arr = [10, 11, 12]
  assert arr[0] == 10
  assert arr[1] == 11
  arr[2] = 100
  assert arr[2] == 100

  arr = [10, 11, 12]
  var i = 0
  assert arr[i] == 10
  i = 2
  assert arr[i] == 12
  arr[i] = 0
  assert arr[i] == 0
}
index_test()
